  "description": "",
  "main": "./dist/index.js",
  "dependencies": {
    "flow-bin": "^0.69.0",
    "flow-remove-types": "^1.2.3",
    "mustache": "^2.3.0",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "pre-commit": "^1.2.2"
  },
  "scripts": {
    "build": "flow-remove-types -a ./src/ -d ./dist/",
